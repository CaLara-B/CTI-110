
# Branching
age = 65

# Using if statement
if age > 65:
    print("Senior Citizen")

# Using if/else statement
if age > 65:
    print("Senior Citizen")
else:
    print("NOT Senior Citizen")
    

# Using if/elif statement
if age > 0:
    print("Baby")
    
if age >= 5:
    print("Child")

if age >= 13:
    print("Teen")
    
if age >= 18:
    print("Adult")
    
if age >= 65:
    print("Senior")
    
else:
    print("Invalid Age")
